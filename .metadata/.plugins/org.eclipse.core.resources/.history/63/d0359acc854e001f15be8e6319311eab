package com.TestNGDemos;

import org.testng.annotations.Test;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.DataProvider;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.AfterTest;

public class D06DataProviderDemo {
	
	@Test(dataProvider = "getLoginData")
	public void loginToOHRM(String un, String ps) {
		System.out.println(un + "\t" + ps);
	}

	@DataProvider
	public Object[][] getLoginData() {
		return new Object[][] {
			new Object[] { "admin", "admin123", 1 },
			new Object[] { "mythili", "mythili123", 2 },
			new Object[] { "admin", "admin123", 3 },
			new Object[] { "bhup", "bhup123", 4 },
			
		};
	}
	
	@AfterMethod
	public void afterMethod() {
	}
	
	@BeforeTest
	public void beforeTest() {
	}

	@AfterTest
	public void afterTest() {
	}

}
